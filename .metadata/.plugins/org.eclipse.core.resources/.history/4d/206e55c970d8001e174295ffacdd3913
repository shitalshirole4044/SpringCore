package com.springcore;

public class Student {
 private int id;
 private String name;
 private String address;
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getAddesss() {
	return address;
}
public void setAddesss(String address) {
	this.address = address;
}
public Student(int id, String name, String addesss) {
	super();
	this.id = id;
	this.name = name;
	this.address = addesss;
}
public Student() {
	super();
}
@Override
public String toString() {
	return "Student [id=" + id + ", name=" + name + ", addesss=" + address + "]";
}

 
}
